{
  "manifest_version": 3,
  "name": "Medialobbyシフト時間自動入力",
  "description": "Medialobbyのシフト時間を自動入力します。",
  "version": "1.0",
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "48": "lobbyber.png"
  },
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["https://docs.google.com/forms/d/e/1FAIpQLScXjIOVl91V7caaoD8MI7_NKAWfQrHgxfq55dPZ2w5V-nAFXQ/"],
  "commands": {
    "calcWorkTime": {
      "suggested_key": {
        "default": "Ctrl+Shift+1",
        "mac": "Command+Shift+1"
      },
      "description": "Calc Actual Work Time at section of day"
    }
  }
}